{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::156460612806:root"
        },
        "Action": "s3:PutObject",
        "Resource": "arn:aws:s3:::devopschallenge-lb-logs/devopschallenge-lb/AWSLogs/836668077738/*"
      },
      {
        "Effect": "Allow",
        "Principal": {
          "Service": "delivery.logs.amazonaws.com"
        },
        "Action": "s3:PutObject",
        "Resource": "arn:aws:s3:::devopschallenge-lb-logs/devopschallenge-lb/AWSLogs/836668077738/*",
        "Condition": {
          "StringEquals": {
            "s3:x-amz-acl": "bucket-owner-full-control"
          }
        }
      },
      {
        "Effect": "Allow",
        "Principal": {
          "Service": "delivery.logs.amazonaws.com"
        },
        "Action": "s3:GetBucketAcl",
        "Resource": "arn:aws:s3:::devopschallenge-lb-logs"
      }
    ]
  }